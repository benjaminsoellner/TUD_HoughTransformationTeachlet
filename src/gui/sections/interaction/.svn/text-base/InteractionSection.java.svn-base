package gui.sections.interaction;

import gui.GuiManager;
import gui.sections.Section;

/**
 * An InteractionSection is that part of the applet, where all the interesting stuff happens.
 * For exemple the HoughTransformationSection or the ImageLoadSection ar both InteractionSections.
 * This class is actually the abstract container for all these sections.
 *
 */
public abstract class InteractionSection extends Section {
	
	/**
	 * The standard constructor for the InteractionSecton just calls the super constructor.
	 */
	public InteractionSection() {
		super();
	}

	private static final long serialVersionUID = 1L;

	/**
	 * The standard constructor for the InteractionSecton just calls the super constructor
	 * with the given parameter guiManager.
	 * @param guiManager
	 */
	public InteractionSection(GuiManager guiManager) {
		super(guiManager);
	}

}
